{
  "name": "fashion-hub",
  "version": "1.0.0",
  "description": "MERN Stack E-Commerce Website for Clothing Store",
  "main": "index.js",
  "scripts": {
    "start": "set PORT=3001 && set NODE_OPTIONS=--openssl-legacy-provider && react-scripts --max_old_space_size=4096 start",
    "build": "set NODE_OPTIONS=--openssl-legacy-provider && react-scripts build",
    "test": "set NODE_OPTIONS=--openssl-legacy-provider && react-scripts test",
    "eject": "set NODE_OPTIONS=--openssl-legacy-provider && react-scripts eject",
    "server": "set NODE_OPTIONS=--openssl-legacy-provider && nodemon backend/server.js",
    "client": "set PORT=3001 && set NODE_OPTIONS=--openssl-legacy-provider && react-scripts --max_old_space_size=4096 start",
    "dev": "set NODE_OPTIONS=--openssl-legacy-provider && concurrently \"npm run server\" \"npm run client\"",
    "data:import": "set NODE_OPTIONS=--openssl-legacy-provider && node backend/seeder",
    "data:destroy": "set NODE_OPTIONS=--openssl-legacy-provider && node backend/seeder -d"
  },
  "proxy": "http://localhost:5002",
  "dependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.6.7",
    "bcryptjs": "^2.4.3",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.2.0",
    "nodemon": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.2",
    "react-scripts": "^3.0.1",
    "web-vitals": "^2.1.4"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
